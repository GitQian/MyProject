<resources xmlns:android="http://schemas.android.com/apk/res/android">
<style name="proDialogStyle">
        <item name="android:windowBackground">@color/bigTransparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <!-- 设置了 android:windowIsFloating=true 就不需要去设置屏幕亮度了 -->
    <style name="translucentDialogStyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/allTransparent</item>
        <item name="android:windowEnterAnimation">@anim/bottom_in</item>
        <item name="android:windowExitAnimation">@anim/bottom_out</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:background">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="pregnancyProDialogStyle" parent="translucentDialogStyle">
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="allTranslucentDialogStyle">
        <item name="android:windowBackground">@color/allTransparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:background">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="exitAnimDialog">
        <item name="android:windowEnterAnimation">@anim/alpha_in</item>
        <item name="android:windowExitAnimation">@anim/alpha_out</item>
    </style>

    <style name="bottomAnimDialog">
        <item name="android:windowEnterAnimation">@anim/bottom_in</item>
        <item name="android:windowExitAnimation">@anim/bottom_out</item>
    </style>

    <style name="exitAllAnimDialog">
        <item name="android:windowEnterAnimation">@anim/bottom_in</item>
        <item name="android:windowExitAnimation">@anim/top_out</item>
    </style>

    <style name="list">
        <item name="android:maxHeight">200dp</item>
    </style>

    <style name="textNormal">
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:textColor">#000</item>
        <item name="android:padding">@dimen/size5</item>
    </style>

    <style name="textBlue">
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:textColor">@color/text_blue</item>
        <item name="android:padding">@dimen/size5</item>
    </style>

    <style name="textBlack">
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:textColor">#000</item>
        <item name="android:padding">@dimen/size5</item>
    </style>

    <style name="PopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/anim_dialog_show</item>
        <item name="android:windowExitAnimation">@anim/anim_dialog_hide</item>
    </style>
    <style name="PopupScaleAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/anim_dialog_show_1</item>
        <item name="android:windowExitAnimation">@anim/anim_dialog_hide_1</item>
    </style>

    <style name="PopupTopAnimation">
        <item name="android:windowEnterAnimation">@anim/top_in</item>
        <item name="android:windowExitAnimation">@anim/top_out</item>
    </style>

    <style name="tvBlue">
        <item name="android:textColor">@color/text_blue1</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="tv555">
        <item name="android:textColor">#555555</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="tv555_16">
