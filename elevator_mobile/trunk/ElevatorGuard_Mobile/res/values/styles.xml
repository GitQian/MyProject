    </style>

    <style name="Theme.YTZT.Splash" parent="@style/Theme.YTZT">
        <item name="android:windowBackground">@color/background_all</item>
    </style>

    <style name="Theme.YTZT" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowAnimationStyle">@style/Animation.Activity.YTZT</item>
    </style>
    <style name="Animation.Activity.YTZT" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/activity_right_in</item>
        <item name="android:activityOpenExitAnimation">@anim/activity_alpha_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/activity_alpha_in</item>
        <item name="android:activityCloseExitAnimation">@anim/activity_right_out</item>
        <item name="android:taskOpenEnterAnimation">@anim/activity_right_in</item>
        <item name="android:taskOpenExitAnimation">@anim/activity_alpha_out</item>
        <item name="android:taskCloseEnterAnimation">@anim/activity_alpha_in</item>
        <item name="android:taskCloseExitAnimation">@anim/activity_right_out</item>
        <item name="android:taskToFrontEnterAnimation">@anim/activity_right_in</item>
        <item name="android:taskToFrontExitAnimation">@anim/activity_alpha_out</item>
        <item name="android:taskToBackEnterAnimation">@anim/activity_alpha_in</item>
        <item name="android:taskToBackExitAnimation">@anim/activity_right_out</item>
    </style>

    <!-- 标题栏 -->
    <style name="common_layout">
        <item name="android:background">@color/title_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/size50</item>
    </style>

    <!-- 标题栏后退按钮 ,已经换成imageview在下面-->
    <style name="common_btn_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">@dimen/size12</item>
        <item name="android:paddingRight">@dimen/size12</item>
        <item name="android:drawableLeft">@drawable/top_back_selector</item>
        <item name="android:drawablePadding">@dimen/size5</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <!-- 顶部标题栏后退imageview -->
    
    <style name="common_iv_back_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:paddingLeft">@dimen/size10</item>
        <item name="android:paddingRight">@dimen/size10</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:src">@drawable/top_back_selector</item>
    </style>
     <!-- 标题栏中间 TextView -->
    <style name="common_title_text">
        <item name="android:textSize">@dimen/textSize20</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <!-- 标题栏右边按钮 -->
    <style name="common_btn_right">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:paddingLeft">@dimen/size10</item>
        <item name="android:paddingRight">@dimen/size10</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:textColor">@color/text_click_wr</item>
        
    </style>
    
      <!-- 除margin外占全屏的大按钮 -->
    <style name="common_btn_bottom">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/size50</item>
        <item name="android:layout_marginLeft">@dimen/size12</item>
        <item name="android:layout_marginRight">@dimen/size12</item>
        <item name="android:layout_marginTop">@dimen/size20</item>
        <item name="android:layout_marginBottom">@dimen/size15</item>
        <item name="android:background">@drawable/btn_radius_blue_gray</item>
        <item name="android:textSize">@dimen/textSize18</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="AppBaseTheme" parent="@android:style/Theme.Light.NoTitleBar"></style>

