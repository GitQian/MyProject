package asmack.bean;

import java.util.ArrayList;

/**
 * 聊天室。 这种结果用expandlistview
 * @author huajian.zhang
 */
public class ChatRoomGroup {

	// 聊天室服务名
	private String serverName;

	// 聊天室房间列表
	private ArrayList<ChatRoomVo> roomList;

	public ChatRoomGroup() {
		// TODO Auto-generated constructor stub
	}

	public ChatRoomGroup(String serverName, ArrayList<ChatRoomVo> roomList) {
		super();
		this.serverName = serverName;
		this.roomList = roomList;
	}

	public String getServerName() {
		return serverName;
	}

	public void setServerName(String serverName) {
		this.serverName = serverName;
	}

	public ArrayList<ChatRoomVo> getRoomList() {
		return roomList;
	}

	public void setRoomList(ArrayList<ChatRoomVo> roomList) {
		this.roomList = roomList;
	}

	@Override
	public String toString() {
		return "ChatRoomGroup [serverName=" + serverName + ", roomList="
				+ roomList + "]";
	}

	
	
}
