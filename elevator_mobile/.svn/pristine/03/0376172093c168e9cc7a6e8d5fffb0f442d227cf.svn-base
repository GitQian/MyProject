package com.chinacnit.elevatorguard.mobile.http.request.params;

import com.chinacnit.elevatorguard.mobile.http.request.HttpBaseRequestParams;

/**
 * 公司详情查询参数
 * 
 * @author ssu
 * @date 2015-6-5 下午8:44:36
 */
public class CompanyDetailParams extends HttpBaseRequestParams {

	/**
	 * 公司id
	 */
	private long companyId;

	/**
	 * 公司类别 1 维保公司; 2 物业公司
	 */
	private int companyType;

	public CompanyDetailParams() {
		super();
	}

	public CompanyDetailParams(long companyId, int companyType) {
		super();
		this.setCompanyId(companyId);
		this.companyType = companyType;
	}

	public long getCompanyId() {
		return companyId;
	}

	public void setCompanyId(long companyId) {
		this.companyId = companyId;
	}

	public int getCompanyType() {
		return companyType;
	}

	public void setCompanyType(int companyType) {
		this.companyType = companyType;
	}
}
