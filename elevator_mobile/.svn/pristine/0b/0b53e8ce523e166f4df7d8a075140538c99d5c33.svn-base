
package com.chinacnit.elevatorguard.core.bean;


public class RefreshInfo {

    /**
     * 当前页的平均数目
     */
    public int avgpage = 10;

    /**
     * 当前页码
     */
    public int page = 1;

    /**
     * 是否需要刷新
     */
    public boolean refresh = true;

    public int getAvgpage() {
        return avgpage;
    }

    public void setAvgpage(int avgpage) {
        this.avgpage = avgpage;
    }

    public int getPage() {
        return page;
    }

    public void setPage(int page) {
        this.page = page;
    }

    public boolean isRefresh() {
        return refresh;
    }

    public void setRefresh(boolean refresh) {
        this.refresh = refresh;
    }

}
