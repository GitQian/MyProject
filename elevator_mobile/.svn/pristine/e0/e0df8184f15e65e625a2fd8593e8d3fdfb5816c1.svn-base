package com.chinacnit.elevatorguard.mobile.http.request.params;

import com.chinacnit.elevatorguard.mobile.http.request.HttpBaseRequestParams;

/**
 * 维保详情请求参数
 * 
 * @author ssu
 * @date 2015-6-15 上午11:06:50
 */
public class WeiBaoDetailParams extends HttpBaseRequestParams {
	/** 计划编号 */
	private long planId;

	public WeiBaoDetailParams() {
		super();
	}

	public WeiBaoDetailParams(long planId) {
		super();
		this.setPlanId(planId);
	}

	public long getPlanId() {
		return planId;
	}

	public void setPlanId(long planId) {
		this.planId = planId;
	}

}
